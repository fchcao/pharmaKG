# Production environment variables for PharmaKG Frontend
# API endpoint will be set via Docker build argument

# Default fallback (will be overridden by docker build)
VITE_API_BASE_URL=http://localhost:8000

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false

# Application settings
VITE_APP_NAME=PharmaKG
VITE_APP_VERSION=1.0.0
