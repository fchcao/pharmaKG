#===========================================================
# 制药行业知识图谱 - 关系语义细化与谓词体系
# Pharmaceutical Knowledge Graph - Relationship Semantics & Predicate System
#===========================================================
# 版本: v1.0
# 创建日期: 2025-02-06
# 描述: 定义跨四大领域的精细化关系语义，包括谓词体系、
#       关系属性、约束规则和推理基础
#===========================================================

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix pharma: <http://pharmakg.org/ontology/> .
@prefix rd: <http://pharmakg.org/ontology/rd/> .
@prefix clinical: <http://pharmakg.org/ontology/clinical/> .
@prefix sc: <http://pharmakg.org/ontology/sc/> .
@prefix regulatory: <http://pharmakg.org/ontology/regulatory/> .
@prefix rel: <http://pharmakg.org/ontology/relationship/> .

#===========================================================
# 本体元数据
#===========================================================

pharma:RelationshipSemantics a owl:Ontology ;
    rdfs:label "PharmaKG Relationship Semantics and Predicate System" ;
    rdfs:comment "精细化关系语义定义，覆盖R&D、临床、供应链、监管四大领域" ;
    owl:versionInfo "v1.0" ;
    owl:versionIRI <http://pharmakg.org/ontology/relationship/1.0> ;
    dc:date "2025-02-06"^^xsd:date ;
    dc:creator "PharmaKG Project Team" .

#===========================================================
# 关系属性通用定义
#===========================================================

# 关系强度
rel:has_strength a owl:DatatypeProperty ;
    rdfs:label "has strength" ;
    rdfs:comment "关系的强度或置信度 (0.0-1.0)" ;
    rdfs:range xsd:decimal .

# 关系证据级别
rel:evidence_level a owl:DatatypeProperty ;
    rdfs:label "evidence level" ;
    rdfs:comment "证据级别: A(强)、B(中)、C(弱)、D(推测)" ;
    rdfs:range xsd:string .

# 关系时态信息
rel:start_date a owl:DatatypeProperty ;
    rdfs:label "start date" ;
    rdfs:comment "关系生效开始日期" ;
    rdfs:range xsd:date .

rel:end_date a owl:DatatypeProperty ;
    rdfs:label "end date" ;
    rdfs:comment "关系生效结束日期" ;
    rdfs:range xsd:date .

# 关系数据源
rel:source_database a owl:DatatypeProperty ;
    rdfs:label "source database" ;
    rdfs:comment "关系来源数据库" ;
    rdfs:range xsd:string .

# 关系来源文献
rel:source_publication a owl:DatatypeProperty ;
    rdfs:label "source publication" ;
    rdfs:comment "关系来源文献DOI或PMID" ;
    rdfs:range xsd:string .

# 关系量化指标
rel:ic50 a owl:DatatypeProperty ;
    rdfs:label "IC50" ;
    rdfs:comment "半抑制浓度 (nM)" ;
    rdfs:range xsd:decimal .

rel:ki a owl:DatatypeProperty ;
    rdfs:label "Ki" ;
    rdfs:comment "抑制常数 (nM)" ;
    rdfs:range xsd:decimal .

rel:ec50 a owl:DatatypeProperty ;
    rdfs:label "EC50" ;
    rdfs:comment "半有效浓度 (nM)" ;
    rdfs:range xsd:decimal .

rel:dose a owl:DatatypeProperty ;
    rdfs:label "dose" ;
    rdfs:comment "剂量 (mg)" ;
    rdfs:range xsd:decimal .

rel:frequency a owl:DatatypeProperty ;
    rdfs:label "frequency" ;
    rdfs:comment "发生频率或次数" ;
    rdfs:range xsd:integer .

#===========================================================
# 一、R&D 领域关系定义
#===========================================================

#-----------------------------------------------------------
# 1.1 化合物-靶点关系 (Compound-Target Relationships)
#-----------------------------------------------------------

rel:INHIBITS a owl:ObjectProperty ;
    rdfs:label "inhibits" ;
    rdfs:comment "化合物抑制靶点的活性" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:interacts_with_target ;
    owl:inverseOf rel:INHIBITED_BY ;
    rel:evidence_level "A" ;
    rdfs:seeAlso <https://www.ebi.ac.uk/chembl/api/data/schema/> .

rel:INHIBITED_BY a owl:ObjectProperty ;
    rdfs:label "inhibited by" ;
    rdfs:comment "靶点被化合物抑制" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:INHIBITS .

rel:ACTIVATES a owl:ObjectProperty ;
    rdfs:label "activates" ;
    rdfs:comment "化合物激活或增强靶点的活性" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:interacts_with_target ;
    owl:inverseOf rel:ACTIVATED_BY ;
    rel:evidence_level "A" .

rel:ACTIVATED_BY a owl:ObjectProperty ;
    rdfs:label "activated by" ;
    rdfs:comment "靶点被化合物激活" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:ACTIVATES .

rel:BINDS_TO a owl:ObjectProperty ;
    rdfs:label "binds to" ;
    rdfs:comment "化合物与靶点物理结合，但不明确激活或抑制" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:interacts_with_target ;
    owl:inverseOf rel:BINDS_COMPOUND ;
    rel:evidence_level "B" .

rel:BINDS_COMPOUND a owl:ObjectProperty ;
    rdfs:label "binds compound" ;
    rdfs:comment "靶点与化合物结合" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:BINDS_TO .

rel:MODULATES a owl:ObjectProperty ;
    rdfs:label "modulates" ;
    rdfs:comment "化合物调节靶点活性（可能是双向调节）" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:interacts_with_target ;
    owl:inverseOf rel:MODULATED_BY ;
    rel:evidence_level "C" .

rel:MODULATED_BY a owl:ObjectProperty ;
    rdfs:label "modulated by" ;
    rdfs:comment "靶点被化合物调节" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:MODULATES .

rel:AGONIST_OF a owl:ObjectProperty ;
    rdfs:label "agonist of" ;
    rdfs:comment "化合物是靶点的激动剂" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:ACTIVATES ;
    owl:inverseOf rel:HAS_AGONIST .

rel:HAS_AGONIST a owl:ObjectProperty ;
    rdfs:label "has agonist" ;
    rdfs:comment "靶点的激动剂" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:AGONIST_OF .

rel:ANTAGONIST_OF a owl:ObjectProperty ;
    rdfs:label "antagonist of" ;
    rdfs:comment "化合物是靶点的拮抗剂" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:INHIBITS ;
    owl:inverseOf rel:HAS_ANTAGONIST .

rel:HAS_ANTAGONIST a owl:ObjectProperty ;
    rdfs:label "has antagonist" ;
    rdfs:comment "靶点的拮抗剂" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:ANTAGONIST_OF .

rel:ALLOSTERIC_MODULATOR_OF a owl:ObjectProperty ;
    rdfs:label "allosteric modulator of" ;
    rdfs:comment "化合物是靶点的变构调节剂" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:MODULATES ;
    owl:inverseOf rel:HAS_ALLOSTERIC_MODULATOR .

rel:PARTIAL_AGONIST_OF a owl:ObjectProperty ;
    rdfs:label "partial agonist of" ;
    rdfs:comment "化合物是靶点的部分激动剂" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:AGONIST_OF .

rel:COMPETITIVE_INHIBITOR_OF a owl:ObjectProperty ;
    rdfs:label "competitive inhibitor of" ;
    rdfs:comment "化合物是靶点的竞争性抑制剂" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:INHIBITS .

rel:NONCOMPETITIVE_INHIBITOR_OF a owl:ObjectProperty ;
    rdfs:label "non-competitive inhibitor of" ;
    rdfs:comment "化合物是靶点的非竞争性抑制剂" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    rdfs:subPropertyOf rel:INHIBITS .

#-----------------------------------------------------------
# 1.2 靶点-通路关系 (Target-Pathway Relationships)
#-----------------------------------------------------------

rel:PARTICIPATES_IN a owl:ObjectProperty ;
    rdfs:label "participates in" ;
    rdfs:comment "靶点参与某生物学通路" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    owl:inverseOf rel:HAS_PARTICIPANT ;
    rel:evidence_level "A" .

rel:HAS_PARTICIPANT a owl:ObjectProperty ;
    rdfs:label "has participant" ;
    rdfs:comment "通路包含该靶点作为参与者" ;
    rdfs:domain rd:Pathway ;
    rdfs:range rd:Target ;
    owl:inverseOf rel:PARTICIPATES_IN .

rel:REGULATES_PATHWAY a owl:ObjectProperty ;
    rdfs:label "regulates pathway" ;
    rdfs:comment "靶点调节通路的活性" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    owl:inverseOf rel:REGULATED_BY_TARGET ;
    rel:evidence_level "A" .

rel:REGULATED_BY_TARGET a owl:ObjectProperty ;
    rdfs:label "regulated by target" ;
    rdfs:comment "通路被靶点调节" ;
    rdfs:domain rd:Pathway ;
    rdfs:range rd:Target ;
    owl:inverseOf rel:REGULATES_PATHWAY .

rel:UPREGULATES a owl:ObjectProperty ;
    rdfs:label "upregulates" ;
    rdfs:comment "靶点上调通路活性" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    rdfs:subPropertyOf rel:REGULATES_PATHWAY .

rel:DOWNREGULATES a owl:ObjectProperty ;
    rdfs:label "downregulates" ;
    rdfs:comment "靶点下调通路活性" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    rdfs:subPropertyOf rel:REGULATES_PATHWAY .

rel:ACTIVATES_PATHWAY a owl:ObjectProperty ;
    rdfs:label "activates pathway" ;
    rdfs:comment "靶点激活通路" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    rdfs:subPropertyOf rel:UPREGULATES .

rel:INHIBITS_PATHWAY a owl:ObjectProperty ;
    rdfs:label "inhibits pathway" ;
    rdfs:comment "靶点抑制通路" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    rdfs:subPropertyOf rel:DOWNREGULATES .

#-----------------------------------------------------------
# 1.3 化合物-疾病关系 (Compound-Disease Relationships)
#-----------------------------------------------------------

rel:TREATS a owl:ObjectProperty ;
    rdfs:label "treats" ;
    rdfs:comment "化合物治疗某种疾病" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:TREATED_BY ;
    rel:evidence_level "A" ;
    rdfs:seeAlso <https://www.drugbank.com/categories> .

rel:TREATED_BY a owl:ObjectProperty ;
    rdfs:label "treated by" ;
    rdfs:comment "疾病被某化合物治疗" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:TREATS .

rel:PREVENTS a owl:ObjectProperty ;
    rdfs:label "prevents" ;
    rdfs:comment "化合物预防某种疾病" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:PREVENTED_BY ;
    rel:evidence_level "B" .

rel:PREVENTED_BY a owl:ObjectProperty ;
    rdfs:label "prevented by" ;
    rdfs:comment "疾病被某化合物预防" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:PREVENTS .

rel:CONTRAINDICATED_FOR a owl:ObjectProperty ;
    rdfs:label "contraindicated for" ;
    rdfs:comment "化合物对某种疾病禁忌使用" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:HAS_CONTRAINDICATION ;
    rel:evidence_level "A" .

rel:HAS_CONTRAINDICATION a owl:ObjectProperty ;
    rdfs:label "has contraindication" ;
    rdfs:comment "疾病对该化合物禁忌" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:CONTRAINDICATED_FOR .

rel:SYMPTOM_OF a owl:ObjectProperty ;
    rdfs:label "symptom of" ;
    rdfs:comment "化合物是某种疾病的症状或副作用" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:CAUSES_SYMPTOM ;
    rel:evidence_level "B" .

rel:CAUSES_SYMPTOM a owl:ObjectProperty ;
    rdfs:label "causes symptom" ;
    rdfs:comment "疾病被化合物引起的症状" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:SYMPTOM_OF .

rel:ASSOCIATED_WITH_TREATMENT a owl:ObjectProperty ;
    rdfs:label "associated with treatment" ;
    rdfs:comment "化合物与疾病治疗相关联（证据较弱）" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:TREATMENT_ASSOCIATION ;
    rel:evidence_level "D" .

rel:TREATMENT_ASSOCIATION a owl:ObjectProperty ;
    rdfs:label "treatment association" ;
    rdfs:comment "疾病与化合物治疗相关联" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:ASSOCIATED_WITH_TREATMENT .

rel:POTENTIALLY_TREATS a owl:ObjectProperty ;
    rdfs:label "potentially treats" ;
    rdfs:comment "化合物可能治疗某种疾病（基于推理）" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    rdfs:subPropertyOf rel:ASSOCIATED_WITH_TREATMENT ;
    rel:evidence_level "D" .

#-----------------------------------------------------------
# 1.4 靶点-疾病关系 (Target-Disease Relationships)
#-----------------------------------------------------------

rel:ASSOCIATED_WITH_DISEASE a owl:ObjectProperty ;
    rdfs:label "associated with disease" ;
    rdfs:comment "靶点与疾病相关联" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:HAS_ASSOCIATED_TARGET ;
    rel:evidence_level "B" .

rel:HAS_ASSOCIATED_TARGET a owl:ObjectProperty ;
    rdfs:label "has associated target" ;
    rdfs:comment "疾病相关的靶点" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Target ;
    owl:inverseOf rel:ASSOCIATED_WITH_DISEASE .

rel:BIOMARKER_FOR a owl:ObjectProperty ;
    rdfs:label "biomarker for" ;
    rdfs:comment "靶点是疾病的生物标志物" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Disease ;
    rdfs:subPropertyOf rel:ASSOCIATED_WITH_DISEASE ;
    owl:inverseOf rel:HAS_BIOMARKER ;
    rel:evidence_level "A" .

rel:HAS_BIOMARKER a owl:ObjectProperty ;
    rdfs:label "has biomarker" ;
    rdfs:comment "疾病的生物标志物" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Target ;
    owl:inverseOf rel:BIOMARKER_FOR .

rel:CAUSES_DISEASE a owl:ObjectProperty ;
    rdfs:label "causes disease" ;
    rdfs:comment "靶点异常导致疾病" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Disease ;
    rdfs:subPropertyOf rel:ASSOCIATED_WITH_DISEASE ;
    owl:inverseOf rel:CAUSED_BY_TARGET ;
    rel:evidence_level "A" .

rel:CAUSED_BY_TARGET a owl:ObjectProperty ;
    rdfs:label "caused by target" ;
    rdfs:comment "疾病由靶点异常导致" ;
    rdfs:domain rd:Disease ;
    rdfs:range rd:Target ;
    owl:inverseOf rel:CAUSES_DISEASE .

#-----------------------------------------------------------
# 1.5 实验关系 (Assay Relationships)
#-----------------------------------------------------------

rel:TESTS_COMPOUND a owl:ObjectProperty ;
    rdfs:label "tests compound" ;
    rdfs:comment "实验测试化合物" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:TESTED_IN_ASSAY .

rel:TESTED_IN_ASSAY a owl:ObjectProperty ;
    rdfs:label "tested in assay" ;
    rdfs:comment "化合物在实验中测试" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Assay ;
    owl:inverseOf rel:TESTS_COMPOUND .

rel:TESTS_TARGET a owl:ObjectProperty ;
    rdfs:label "tests target" ;
    rdfs:comment "实验测试靶点" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:Target ;
    owl:inverseOf rel:TESTED_IN_ASSAY_TARGET .

rel:TESTED_IN_ASSAY_TARGET a owl:ObjectProperty ;
    rdfs:label "tested in assay (target)" ;
    rdfs:comment "靶点在实验中测试" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Assay ;
    owl:inverseOf rel:TESTS_TARGET .

rel:HAS_ASSAY_RESULT a owl:ObjectProperty ;
    rdfs:label "has assay result" ;
    rdfs:comment "实验产生结果" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:AssayResult ;
    owl:inverseOf rel:RESULT_OF_ASSAY .

rel:RESULT_OF_ASSAY a owl:ObjectProperty ;
    rdfs:label "result of assay" ;
    rdfs:comment "结果是某实验的" ;
    rdfs:domain rd:AssayResult ;
    rdfs:range rd:Assay ;
    owl:inverseOf rel:HAS_ASSAY_RESULT .

rel:HAS_SAR_DATA a owl:ObjectProperty ;
    rdfs:label "has SAR data" ;
    rdfs:comment "化合物具有构效关系数据" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:SARData ;
    owl:inverseOf rel:SAR_DATA_FOR .

rel:SAR_DATA_FOR a owl:ObjectProperty ;
    rdfs:label "SAR data for" ;
    rdfs:comment "SAR数据对应的化合物" ;
    rdfs:domain rd:SARData ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:HAS_SAR_DATA .

rel:IMPROVES_POTENCY a owl:ObjectProperty ;
    rdfs:label "improves potency" ;
    rdfs:comment "SAR修饰改善化合物效力" ;
    rdfs:domain rd:SARData ;
    rdfs:range rd:Compound .

rel:REDUCES_TOXICITY a owl:ObjectProperty ;
    rdfs:label "reduces toxicity" ;
    rdfs:comment "SAR修饰降低化合物毒性" ;
    rdfs:domain rd:SARData ;
    rdfs:range rd:Compound .

rel:IMPROVES_SELECTIVITY a owl:ObjectProperty ;
    rdfs:label "improves selectivity" ;
    rdfs:comment "SAR修饰改善化合物选择性" ;
    rdfs:domain rd:SARData ;
    rdfs:range rd:Compound .

#===========================================================
# 二、临床领域关系定义
#===========================================================

#-----------------------------------------------------------
# 2.1 试验-受试者关系 (Trial-Subject Relationships)
#-----------------------------------------------------------

rel:ENROLS a owl:ObjectProperty ;
    rdfs:label "enrols" ;
    rdfs:comment "试验招募受试者" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:Subject ;
    owl:inverseOf rel:ENROLLED_IN ;
    rel:evidence_level "A" .

rel:ENROLLED_IN a owl:ObjectProperty ;
    rdfs:label "enrolled in" ;
    rdfs:comment "受试者被招募到试验中" ;
    rdfs:domain clinical:Subject ;
    rdfs:range clinical:ClinicalTrial ;
    owl:inverseOf rel:ENROLS .

rel:EXCLUDES a owl:ObjectProperty ;
    rdfs:label "excludes" ;
    rdfs:comment "试验排除某受试者（不符合入组标准）" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:Subject ;
    owl:inverseOf rel:EXCLUDED_FROM .

rel:EXCLUDED_FROM a owl:ObjectProperty ;
    rdfs:label "excluded from" ;
    rdfs:comment "受试者被试验排除" ;
    rdfs:domain clinical:Subject ;
    rdfs:range clinical:ClinicalTrial ;
    owl:inverseOf rel:EXCLUDES .

rel:WITHDREW_FROM a owl:ObjectProperty ;
    rdfs:label "withdrew from" ;
    rdfs:comment "受试者退出试验" ;
    rdfs:domain clinical:Subject ;
    rdfs:range clinical:ClinicalTrial ;
    rel:evidence_level "A" .

rel:COMPLETED_TRIAL a owl:ObjectProperty ;
    rdfs:label "completed trial" ;
    rdfs:comment "受试者完成试验" ;
    rdfs:domain clinical:Subject ;
    rdfs:range clinical:ClinicalTrial ;
    rel:evidence_level "A" .

rel:ASSIGNED_TO_ARM a owl:ObjectProperty ;
    rdfs:label "assigned to arm" ;
    rdfs:comment "受试者被分配到某试验组" ;
    rdfs:domain clinical:Subject ;
    rdfs:range clinical:Arm ;
    owl:inverseOf rel:HAS_SUBJECT .

rel:HAS_SUBJECT a owl:ObjectProperty ;
    rdfs:label "has subject" ;
    rdfs:comment "试验组包含受试者" ;
    rdfs:domain clinical:Arm ;
    rdfs:range clinical:Subject ;
    owl:inverseOf rel:ASSIGNED_TO_ARM .

#-----------------------------------------------------------
# 2.2 试验-干预关系 (Trial-Intervention Relationships)
#-----------------------------------------------------------

rel:TESTS_INTERVENTION a owl:ObjectProperty ;
    rdfs:label "tests intervention" ;
    rdfs:comment "试验测试某种干预措施" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:Intervention ;
    owl:inverseOf rel:TESTED_IN_TRIAL ;
    rel:evidence_level "A" .

rel:TESTED_IN_TRIAL a owl:ObjectProperty ;
    rdfs:label "tested in trial" ;
    rdfs:comment "干预措施在试验中测试" ;
    rdfs:domain clinical:Intervention ;
    rdfs:range clinical:ClinicalTrial ;
    owl:inverseOf rel:TESTS_INTERVENTION .

rel:COMPARES_WITH a owl:ObjectProperty ;
    rdfs:label "compares with" ;
    rdfs:comment "试验比较两种干预措施" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:Intervention .

rel:USES_COMPARATOR a owl:ObjectProperty ;
    rdfs:label "uses comparator" ;
    rdfs:comment "试验使用对照干预" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:Intervention ;
    rdfs:subPropertyOf rel:COMPARES_WITH .

rel:USES_PLACEBO a owl:ObjectProperty ;
    rdfs:label "uses placebo" ;
    rdfs:comment "试验使用安慰剂对照" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:Intervention ;
    rdfs:subPropertyOf rel:USES_COMPARATOR .

#-----------------------------------------------------------
# 2.3 受试者-结果关系 (Subject-Outcome Relationships)
#-----------------------------------------------------------

rel:EXPERIENCED_OUTCOME a owl:ObjectProperty ;
    rdfs:label "experienced outcome" ;
