    rdfs:comment "靶点是临床试验中的生物标志物" ;
    rdfs:domain rd:Target ;
    rdfs:range clinical:ClinicalTrial .

#-----------------------------------------------------------
# 5.4 疾病-临床试验跨域关系
#-----------------------------------------------------------

rel:TRIAL_FOR_DISEASE a owl:ObjectProperty ;
