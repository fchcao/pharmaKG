    rdfs:comment "临床试验针对某疾病" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range rd:Disease ;
    owl:inverseOf rel:DISEASE_HAS_TRIAL .

rel:DISEASE_HAS_TRIAL a owl:ObjectProperty ;
    rdfs:label "disease has trial" ;
    rdfs:comment "疾病有相关临床试验" ;
    rdfs:domain rd:Disease ;
