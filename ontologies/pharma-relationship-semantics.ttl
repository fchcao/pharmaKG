rel:TRIAL_TESTED_COMPOUND a owl:ObjectProperty ;
    rdfs:label "trial tested compound" ;
    rdfs:comment "试验测试了某化合物" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range rd:Compound ;
    owl:inverseOf rel:TESTED_IN_CLINICAL_TRIAL .

rel:COMPOUND_IN_INTERVENTION a owl:ObjectProperty ;
    rdfs:label "compound in intervention" ;
    rdfs:comment "化合物是临床试验的干预措施" ;
    rdfs:domain rd:Compound ;
    rdfs:range clinical:Intervention .

#-----------------------------------------------------------
# 5.2 化合物-批准跨域关系
#-----------------------------------------------------------

rel:APPROVED_AS_DRUG a owl:ObjectProperty ;
    rdfs:label "approved as drug" ;
    rdfs:comment "化合物被批准为药物" ;
    rdfs:domain rd:Compound ;
    rdfs:range regulatory:Approval ;
    owl:inverseOf rel:DRUG_APPROVAL_FOR_COMPOUND .

rel:DRUG_APPROVAL_FOR_COMPOUND a owl:ObjectProperty ;
    rdfs:label "drug approval for compound" ;
    rdfs:comment "批准针对某化合物" ;
    rdfs:domain regulatory:Approval ;
    rdfs:range rd:Compound ;
