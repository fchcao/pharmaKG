#===========================================================
# 六、关系互斥与约束定义
#===========================================================

# 互斥关系组定义
rel:MutuallyExclusiveActivation a owl:Class ;
    rdfs:label "Mutually Exclusive Activation" ;
    rdfs:comment "激活类关系的互斥组: INHIBITS vs ACTIVATES" ;
    owl:unionOf ( rel:INHIBITS rel:ACTIVATES ) .

rel:MutuallyExclusiveInspection a owl:Class ;
    rdfs:label "Mutually Exclusive Inspection" ;
    rdfs:comment "检查结果的互斥组: PASSED_INSPECTION vs FAILED_INSPECTION" ;
    owl:unionOf ( rel:PASSED_INSPECTION rel:FAILED_INSPECTION ) .

rel:MutuallyExclusiveTrialStatus a owl:Class ;
    rdfs:label "Mutually Exclusive Trial Status" ;
    rdfs:comment "试验状态的互斥组: COMPLETED vs WITHDREW" ;
    owl:unionOf ( rel:COMPLETED_TRIAL rel:WITHDREW_FROM ) .

#===========================================================
# 七、关系传递性定义
#===========================================================

rel:transitive a owl:AnnotationProperty ;
    rdfs:label "transitive" ;
    rdfs:comment "标注关系是否可传递" ;
    rdfs:range xsd:boolean .

rel:PART_OF a owl:ObjectProperty ;
    rdfs:label "part of" ;
    rdfs:comment "部分-整体关系，可传递" ;
    rdf:type owl:TransitiveProperty ;
    rel:transitive true .

rel:LOCATED_IN a owl:ObjectProperty ;
    rdfs:label "located in" ;
    rdfs:comment "位置关系，可传递" ;
    rdf:type owl:TransitiveProperty ;
    rel:transitive true .

rel:TREATS a owl:ObjectProperty ;
    rel:transitive false .

rel:INHIBITS a owl:ObjectProperty ;
    rel:transitive false .

rel:MANUFACTURES a owl:ObjectProperty ;
    rel:transitive false .

#===========================================================
# 八、推理规则提示（实际实现使用SHACL或SPIN）
#===========================================================

# 推理规则: 化合物抑制与疾病相关的靶点 → 可能治疗该疾病
rel:POTENTIALLY_TREATS a owl:ObjectProperty ;
    rdfs:label "potentially treats (inferred)" ;
    rdfs:comment "推理: 如果化合物INHIBITS靶点，且靶点ASSOCIATED_WITH_DISEASE，则化合物POTENTIALLY_TREATS疾病" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Disease ;
    rel:evidence_level "D" .

# 推理规则: 试验报告严重不良事件 → 产品可能存在安全问题
rel:MAY_HAVE_SAFETY_ISSUE a owl:ObjectProperty ;
    rdfs:label "may have safety issue (inferred)" ;
    rdfs:comment "推理: 如果试验REPORTED_SERIOUS_ADVERSE_EVENT，则化合物MAY_HAVE_SAFETY_ISSUE" ;
    rdfs:domain rd:Compound ;
    rdfs:range regulatory:SafetySignal ;
    rel:evidence_level "D" .

# 推理规则: 制造商多次检查失败 → 可能存在质量问题
rel:POTENTIAL_QUALITY_ISSUE a owl:ObjectProperty ;
    rdfs:label "potential quality issue (inferred)" ;
    rdfs:comment "推理: 如果制造商多次FAILED_INSPECTION，则POTENTIAL_QUALITY_ISSUE" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range sc:DrugShortage ;
    rel:evidence_level "D" .

