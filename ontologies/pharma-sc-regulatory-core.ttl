//===========================================================

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix pharma: <http://pharmakg.org/ontology/> .
@prefix sc: <http://pharmakg.org/ontology/supplychain/> .
@prefix regulatory: <http://pharmakg.org/ontology/regulatory/> .
@prefix sio: <http://semanticscience.org/resource/SIO_> .

#===========================================================
# 本体元数据
#===========================================================

pharma:SupplyChainRegulatoryKG a owl:Ontology ;
    rdfs:label "Pharmaceutical Knowledge Graph - Supply Chain & Regulatory Domain Ontology" ;
    rdfs:comment "供应链与监管合规领域本体，定义制造商、供应商、监管机构、批准等核心实体" ;
    owl:versionInfo "v1.0" ;
    owl:versionIRI <http://pharmakg.org/ontology/sc-regulatory/1.0> ;
    dc:date "2025-02-06"^^xsd:date ;
    dc:creator "PharmaKG Project Team" .

#===========================================================
# ============================================================
# 供应链领域 (Supply Chain Domain)
# ============================================================
#===========================================================

#===========================================================
# 1. Manufacturer (制造商)
#===========================================================

sc:Manufacturer a owl:Class ;
    rdfs:label "Manufacturer" ;
    rdfs:subClassOf pharma:Entity ;
    rdfs:comment "药品制造商，包括原料药和制剂生产商" .

sc:has_manufacturer_id a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "has manufacturer identifier" ;
    rdfs:comment "制造商唯一标识符" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_manufacturer_name a owl:DatatypeProperty ;
    rdfs:label "has manufacturer name" ;
    rdfs:comment "制造商名称" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_manufacturer_type a owl:ObjectProperty ;
    rdfs:label "has manufacturer type" ;
    rdfs:comment "制造商类型" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range sc:ManufacturerType .

sc:has_establishment_id a owl:DatatypeProperty ;
    rdfs:label "has establishment identifier" ;
    rdfs:comment "生产场地标识符（如FDA FEI）" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_fei_code a owl:DatatypeProperty ;
    rdfs:label "has FEI code" ;
    rdfs:comment "FDA Establishment Identifier" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_address a owl:DatatypeProperty ;
    rdfs:label "has address" ;
    rdfs:comment "地址" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_city a owl:DatatypeProperty ;
    rdfs:label "has city" ;
    rdfs:comment "城市" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_state a owl:DatatypeProperty ;
    rdfs:label "has state" ;
    rdfs:comment "州/省" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_country a owl:DatatypeProperty ;
    rdfs:label "has country" ;
    rdfs:comment "国家" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_postal_code a owl:DatatypeProperty ;
    rdfs:label "has postal code" ;
    rdfs:comment "邮政编码" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_website a owl:DatatypeProperty ;
    rdfs:label "has website" ;
    rdfs:comment "网站" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_email a owl:DatatypeProperty ;
    rdfs:label "has email" ;
    rdfs:comment "邮箱" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_phone a owl:DatatypeProperty ;
    rdfs:label "has phone" ;
    rdfs:comment "电话" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:string .

sc:has_manufacturing_capacity a owl:DatatypeProperty ;
    rdfs:label "has manufacturing capacity" ;
    rdfs:comment "生产能力（单位/年）" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range xsd:decimal .

sc:has_certification a owl:ObjectProperty ;
    rdfs:label "has certification" ;
    rdfs:comment "认证资质（如GMP）" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range sc:Certification .

sc:has_regulatory_status a owl:ObjectProperty ;
    rdfs:label "has regulatory status" ;
    rdfs:comment "监管状态" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range regulatory:RegulatoryStatus .

sc:manufactures_drug a owl:ObjectProperty ;
    rdfs:label "manufactures drug" ;
    rdfs:comment "生产的药品" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range rd:Compound .

sc:produces_api a owl:ObjectProperty ;
    rdfs:label "produces API" ;
    rdfs:comment "生产的原料药" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range sc:ActivePharmaceuticalIngredient .

sc:has_facility a owl:ObjectProperty ;
    rdfs:label "has facility" ;
    rdfs:comment "生产设施" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range sc:Facility .

sc:has_inspection a owl:ObjectProperty ;
    rdfs:label "has inspection" ;
    rdfs:comment "检查记录" ;
    rdfs:domain sc:Manufacturer ;
    rdfs:range regulatory:Inspection .

