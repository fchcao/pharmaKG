#===========================================================
# 2. Subject/Patient (受试者/患者)
#===========================================================

clinical:Subject a owl:Class ;
    rdfs:label "Subject" ;
    rdfs:subClassOf pharma:Entity ;
    rdfs:comment "临床试验受试者" ;
    owl:equivalentClass clinical:Patient .

clinical:Patient a owl:Class ;
    rdfs:label "Patient" ;
    rdfs:subClassOf clinical:Subject ;
    rdfs:comment "患者（真实世界数据）" .

