@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix pharma: <http://pharmakg.org/ontology/> .
@prefix clinical: <http://pharmakg.org/ontology/clinical/> .
@prefix sio: <http://semanticscience.org/resource/SIO_> .

#===========================================================
# 本体元数据
#===========================================================

pharma:ClinicalKG a owl:Ontology ;
    rdfs:label "Pharmaceutical Knowledge Graph - Clinical Domain Ontology" ;
    rdfs:comment "临床试验领域本体，定义试验、受试者、干预、结局等核心实体" ;
    owl:versionInfo "v1.0" ;
    owl:versionIRI <http://pharmakg.org/ontology/clinical/1.0> ;
    dc:date "2025-02-06"^^xsd:date ;
    dc:creator "PharmaKG Project Team" .

#===========================================================
# 1. ClinicalTrial (临床试验)
#===========================================================

clinical:ClinicalTrial a owl:Class ;
    rdfs:label "Clinical Trial" ;
    rdfs:subClassOf pharma:Entity ;
    rdfs:comment "临床试验研究" ;
    owl:equivalentClass <http://purl.bioontology.org/ontology/SNOMEDCT/394725008> .  # Clinical trial

# ----- ClinicalTrial 属性 -----

clinical:has_trial_id a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "has trial identifier" ;
    rdfs:comment "试验唯一标识符（如NCT编号）" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range xsd:string .

clinical:has_protocol_id a owl:DatatypeProperty ;
    rdfs:label "has protocol identifier" ;
    rdfs:comment "试验方案编号" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range xsd:string .

clinical:has_trial_title a owl:DatatypeProperty ;
    rdfs:label "has trial title" ;
    rdfs:comment "试验标题" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range xsd:string .

clinical:has_trial_phase a owl:ObjectProperty ;
    rdfs:label "has trial phase" ;
    rdfs:comment "试验阶段" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:TrialPhase .

clinical:has_trial_status a owl:ObjectProperty ;
    rdfs:label "has trial status" ;
    rdfs:comment "试验状态" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:TrialStatus .

clinical:has_study_type a owl:ObjectProperty ;
    rdfs:label "has study type" ;
    rdfs:comment "研究类型" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:StudyType .

clinical:has_study_design a owl:ObjectProperty ;
    rdfs:label "has study design" ;
    rdfs:comment "研究设计" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:StudyDesign .

clinical:has_allocation_type a owl:ObjectProperty ;
    rdfs:label "has allocation type" ;
    rdfs:comment "分配类型（随机化）" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:AllocationType .

clinical:has_masking_type a owl:ObjectProperty ;
    rdfs:label "has masking type" ;
    rdfs:comment "设盲类型" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:MaskingType .

clinical:has_primary_purpose a owl:ObjectProperty ;
