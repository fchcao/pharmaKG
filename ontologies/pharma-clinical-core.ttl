clinical:has_biospec_retention a owl:ObjectProperty ;
    rdfs:label "has biospecimen retention" ;
    rdfs:comment "生物样本保留" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range clinical:BiospecRetention .

clinical:has_biospec_description a owl:DatatypeProperty ;
    rdfs:label "has biospecimen description" ;
    rdfs:comment "生物样本描述" ;
    rdfs:domain clinical:ClinicalTrial ;
    rdfs:range xsd:string .

clinical:has_has_expanded_access a owl:DatatypeProperty ;
    rdfs:label "has expanded access" ;
