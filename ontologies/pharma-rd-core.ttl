#===========================================================
# 制药行业知识图谱 - R&D 核心本体
# Pharmaceutical Knowledge Graph - R&D Core Ontology
#===========================================================
# 版本: v1.0
# 创建日期: 2025-02-06
# 描述: 定义研究与发现(R&D)领域的核心实体类、属性和关系
#===========================================================

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix pharma: <http://pharmakg.org/ontology/> .
@prefix rd: <http://pharmakg.org/ontology/rd/> .
@prefix bmo: <http://purl.obolibrary.org/obo/BMO_> .
@prefix bao: <http://www.bioassayontology.org/bao#> .
@prefix cheminf: <http://semanticscience.org/resource/CHEMINF_> .
@prefix sio: <http://semanticscience.org/resource/SIO_> .

#===========================================================
# 本体元数据
#===========================================================

pharma:PharmaKG_RD a owl:Ontology ;
    rdfs:label "Pharmaceutical Knowledge Graph - R&D Domain Ontology" ;
    rdfs:comment "R&D领域本体，定义化合物、靶点、实验、通路等核心实体" ;
    owl:versionInfo "v1.0" ;
    owl:versionIRI <http://pharmakg.org/ontology/rd/1.0> ;
    dc:date "2025-02-06"^^xsd:date ;
    dc:creator "PharmaKG Project Team" .

#===========================================================
# 抽象基类
#===========================================================

pharma:Entity a owl:Class ;
    rdfs:label "Entity" ;
    rdfs:comment "所有实体的抽象基类" ;
    owl:abstract true .

pharma:has_primary_id a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "has primary identifier" ;
    rdfs:comment "实体的主标识符（唯一标识）" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:string .

pharma:has_name a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:comment "实体的名称" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:string .

pharma:has_alternative_id a owl:DatatypeProperty ;
    rdfs:label "has alternative identifier" ;
    rdfs:comment "实体的备用标识符" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:string .

pharma:has_description a owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:comment "实体的描述信息" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:string .

pharma:created_at a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "created at" ;
    rdfs:comment "实体创建时间戳" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:dateTime .

pharma:updated_at a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "updated at" ;
    rdfs:comment "实体更新时间戳" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:dateTime .

pharma:data_source a owl:DatatypeProperty ;
    rdfs:label "data source" ;
    rdfs:comment "数据来源" ;
    rdfs:domain pharma:Entity ;
    rdfs:range xsd:string .

pharma:has_evidence a owl:ObjectProperty ;
    rdfs:label "has evidence" ;
    rdfs:comment "关联的证据来源" ;
    rdfs:domain pharma:Entity ;
    rdfs:range pharma:Evidence .

#===========================================================
# 1. Compound (化合物)
#===========================================================

rd:Compound a owl:Class ;
    rdfs:label "Compound" ;
    rdfs:subClassOf pharma:Entity ;
    rdfs:comment "化学物质/化合物，包括小分子药物、天然产物等" ;
    owl:equivalentClass cheminf:CHEMINF_000000 ;  # Chemical entity

# ----- Compound 属性 -----

rd:has_smiles a owl:DatatypeProperty ;
    rdfs:label "has SMILES" ;
    rdfs:comment "Simplified Molecular Input Line Entry System (SMILES) 表示" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string ;
    owl:equivalentProperty cheminf:SIO_000018 .

rd:has_inchikey a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "has InChIKey" ;
    rdfs:comment "International Chemical Identifier Key" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string ;
    owl:equivalentProperty cheminf:SIO_000070 .

rd:has_inchi a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:label "has InChI" ;
    rdfs:comment "International Chemical Identifier" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string .

rd:has_molecular_formula a owl:DatatypeProperty ;
    rdfs:label "has molecular formula" ;
    rdfs:comment "分子式" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string .

rd:has_molecular_weight a owl:DatatypeProperty ;
    rdfs:label "has molecular weight" ;
    rdfs:comment "分子量 (Da)" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:decimal .

rd:has_logp a owl:DatatypeProperty ;
    rdfs:label "has LogP" ;
    rdfs:comment "脂水分配系数 (XLogP3)" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:decimal .

rd:has_logS a owl:DatatypeProperty ;
    rdfs:label "has LogS" ;
    rdfs:comment "水溶性 (LogS)" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:decimal .

rd:has_polar_surface_area a owl:DatatypeProperty ;
    rdfs:label "has polar surface area" ;
    rdfs:comment "极性表面积 (TPSA)" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:decimal .

rd:has_rotatable_bonds a owl:DatatypeProperty ;
    rdfs:label "has rotatable bonds" ;
    rdfs:comment "可旋转键数量" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:integer .

rd:has_hbond_donors a owl:DatatypeProperty ;
    rdfs:label "has H-bond donors" ;
    rdfs:comment "氢键供体数量" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:integer .

rd:has_hbond_acceptors a owl:DatatypeProperty ;
    rdfs:label "has H-bond acceptors" ;
    rdfs:comment "氢键受体数量" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:integer .

rd:has_structure_type a owl:ObjectProperty ;
    rdfs:label "has structure type" ;
    rdfs:comment "化合物结构类型" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:StructureType .

rd:has_development_stage a owl:ObjectProperty ;
    rdfs:label "has development stage" ;
    rdfs:comment "开发阶段" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:DevelopmentStage .

rd:has_drug_likeness_score a owl:DatatypeProperty ;
    rdfs:label "has drug-likeness score" ;
    rdfs:comment "药物相似性评分 (如 QED)" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:decimal .

rd:has_lipinski_violations a owl:DatatypeProperty ;
    rdfs:label "has Lipinski violations" ;
    rdfs:comment "Lipinski 五法则违反数量" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:integer .

rd:has_pubchem_cid a owl:DatatypeProperty ;
    rdfs:label "has PubChem CID" ;
    rdfs:comment "PubChem Compound ID" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string .

rd:has_chembl_id a owl:DatatypeProperty ;
    rdfs:label "has ChEMBL ID" ;
    rdfs:comment "ChEMBL 数据库ID" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string .

rd:has_drugbank_id a owl:DatatypeProperty ;
    rdfs:label "has DrugBank ID" ;
    rdfs:comment "DrugBank 数据库ID" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:string .

rd:is_approved_drug a owl:DatatypeProperty ;
    rdfs:label "is approved drug" ;
    rdfs:comment "是否为已批准药物" ;
    rdfs:domain rd:Compound ;
    rdfs:range xsd:boolean .

# ----- Compound 关系 -----

rd:inhibits a owl:ObjectProperty ;
    rdfs:label "inhibits" ;
    rdfs:comment "化合物抑制靶点" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    owl:inverseOf rd:is_inhibited_by .

rd:activates a owl:ObjectProperty ;
    rdfs:label "activates" ;
    rdfs:comment "化合物激活靶点" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    owl:inverseOf rd:is_activated_by .

rd:binds_to a owl:ObjectProperty ;
    rdfs:label "binds to" ;
    rdfs:comment "化合物与靶点结合" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Target ;
    owl:inverseOf rd:is_bound_by .

rd:has_sar_data a owl:ObjectProperty ;
    rdfs:label "has SAR data" ;
    rdfs:comment "化合物的构效关系数据" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:SARData .

rd:is_similar_to a owl:SymmetricProperty ;
    rdfs:label "is similar to" ;
    rdfs:comment "化合物结构相似性" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Compound .

rd:has_derivative a owl:ObjectProperty ;
    rdfs:label "has derivative" ;
    rdfs:comment "化合物的衍生物" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Compound ;
    owl:inverseOf rd:is_derivative_of .

rd:is_derivative_of a owl:ObjectProperty ;
    rdfs:label "is derivative of" ;
    rdfs:comment "是...的衍生物" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Compound .

rd:is_congener_of a owl:SymmetricProperty ;
    rdfs:label "is congener of" ;
    rdfs:comment "是...的同系物" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Compound .

rd:is_prodrug_of a owl:ObjectProperty ;
    rdfs:label "is prodrug of" ;
    rdfs:comment "是...的前药" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Compound .

rd:has_metabolite a owl:ObjectProperty ;
    rdfs:label "has metabolite" ;
    rdfs:comment "化合物的代谢产物" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Compound .

rd:is_part_of_pathway a owl:ObjectProperty ;
    rdfs:label "is part of pathway" ;
    rdfs:comment "化合物参与通路" ;
    rdfs:domain rd:Compound ;
    rdfs:range rd:Pathway .

# ----- Compound 辅助类 -----

rd:StructureType a owl:Class ;
    rdfs:label "Structure Type" ;
    rdfs:comment "化合物结构类型" .

rd:StructureType_small_molecule a rd:StructureType ;
    rdfs:label "Small Molecule" ;
    rdfs:comment "小分子化合物" .

rd:StructureType_peptide a rd:StructureType ;
    rdfs:label "Peptide" ;
    rdfs:comment "肽类化合物" .

rd:StructureType_oligosaccharide a rd:StructureType ;
    rdfs:label "Oligosaccharide" ;
    rdfs:comment "寡糖" .

rd:StructureType_lipid a rd:StructureType ;
    rdfs:label "Lipid" ;
    rdfs:comment "脂类" .

rd:StructureType_natural_product a rd:StructureType ;
    rdfs:label "Natural Product" ;
    rdfs:comment "天然产物" .

rd:DevelopmentStage a owl:Class ;
    rdfs:label "Development Stage" ;
    rdfs:comment "化合物开发阶段" .

rd:DevelopmentStage_hit a rd:DevelopmentStage ;
    rdfs:label "Hit" ;
    rdfs:comment "命中化合物" .

rd:DevelopmentStage_lead a rd:DevelopmentStage ;
    rdfs:label "Lead" ;
    rdfs:comment "先导化合物" .

rd:DevelopmentStage_PCC a rd:DevelopmentStage ;
    rdfs:label "PCC" ;
    rdfs:comment "临床前候选化合物" .

rd:DevelopmentStage_IND a rd:DevelopmentStage ;
    rdfs:label "IND" ;
    rdfs:comment "研究性新药" .

rd:DevelopmentPhase_1 a rd:DevelopmentStage ;
    rdfs:label "Phase 1" ;
    rdfs:comment "临床I期" .

rd:DevelopmentPhase_2 a rd:DevelopmentStage ;
    rdfs:label "Phase 2" ;
    rdfs:comment "临床II期" .

rd:DevelopmentPhase_3 a rd:DevelopmentStage ;
    rdfs:label "Phase 3" ;
    rdfs:comment "临床III期" .

rd:DevelopmentStage_approved a rd:DevelopmentStage ;
    rdfs:label "Approved" ;
    rdfs:comment "已批准" .

rd:DevelopmentStage_withdrawn a rd:DevelopmentStage ;
    rdfs:label "Withdrawn" ;
    rdfs:comment "已撤回" .

#===========================================================
# 2. Target (靶点)
#===========================================================

rd:Target a owl:Class ;
    rdfs:label "Target" ;
    rdfs:subClassOf pharma:Entity ;
    rdfs:comment "药物靶点，包括蛋白质、基因、受体等" .

# ----- Target 属性 -----

rd:has_target_type a owl:ObjectProperty ;
    rdfs:label "has target type" ;
    rdfs:comment "靶点类型" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:TargetType .

rd:has_organism a owl:DatatypeProperty ;
    rdfs:label "has organism" ;
    rdfs:comment "来源生物体" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_gene_name a owl:DatatypeProperty ;
    rdfs:label "has gene name" ;
    rdfs:comment "基因名称" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_gene_symbol a owl:DatatypeProperty ;
    rdfs:label "has gene symbol" ;
    rdfs:comment "基因符号" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_uniprot_id a owl:DatatypeProperty ;
    rdfs:label "has UniProt ID" ;
    rdfs:comment "UniProt 蛋白质数据库ID" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_ensembl_id a owl:DatatypeProperty ;
    rdfs:label "has Ensembl ID" ;
    rdfs:comment "Ensembl 基因组数据库ID" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_entrez_id a owl:DatatypeProperty ;
    rdfs:label "has Entrez ID" ;
    rdfs:comment "Entrez Gene ID" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_refseq_id a owl:DatatypeProperty ;
    rdfs:label "has RefSeq ID" ;
    rdfs:comment "NCBI RefSeq ID" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_protein_sequence a owl:DatatypeProperty ;
    rdfs:label "has protein sequence" ;
    rdfs:comment "蛋白质序列" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_sequence_length a owl:DatatypeProperty ;
    rdfs:label "has sequence length" ;
    rdfs:comment "序列长度（氨基酸数）" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:integer .

rd:has_molecular_weight a owl:DatatypeProperty ;
    rdfs:label "has molecular weight" ;
    rdfs:comment "蛋白质分子量 (kDa)" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:decimal .

rd:has_cellular_location a owl:DatatypeProperty ;
    rdfs:label "has cellular location" ;
    rdfs:comment "细胞定位" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_subcellular_location a owl:DatatypeProperty ;
    rdfs:label "has subcellular location" ;
    rdfs:comment "亚细胞定位" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_go_annotation a owl:DatatypeProperty ;
    rdfs:label "has GO annotation" ;
    rdfs:comment "Gene Ontology 注释" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

rd:has_druggability_score a owl:DatatypeProperty ;
    rdfs:label "has druggability score" ;
    rdfs:comment "可成药性评分" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:decimal .

rd:has_safety_concern a owl:DatatypeProperty ;
    rdfs:label "has safety concern" ;
    rdfs:comment "安全性关注点" ;
    rdfs:domain rd:Target ;
    rdfs:range xsd:string .

# ----- Target 关系 -----

rd:is_inhibited_by a owl:ObjectProperty ;
    rdfs:label "is inhibited by" ;
    rdfs:comment "被化合物抑制" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound .

rd:is_activated_by a owl:ObjectProperty ;
    rdfs:label "is activated by" ;
    rdfs:comment "被化合物激活" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound .

rd:is_bound_by a owl:ObjectProperty ;
    rdfs:label "is bound by" ;
    rdfs:comment "被化合物结合" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Compound .

rd:participates_in a owl:ObjectProperty ;
    rdfs:label "participates in" ;
    rdfs:comment "参与通路" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Pathway ;
    owl:inverseOf rd:includes_target .

rd:regulates a owl:ObjectProperty ;
    rdfs:label "regulates" ;
    rdfs:comment "调控" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Target .

rd:is_regulated_by a owl:ObjectProperty ;
    rdfs:label "is regulated by" ;
    rdfs:comment "被...调控" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Target .

rd:interacts_with a owl:SymmetricProperty ;
    rdfs:label "interacts with" ;
    rdfs:comment "蛋白-蛋白相互作用" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Target .

rd:has_domain a owl:ObjectProperty ;
    rdfs:label "has domain" ;
    rdfs:comment "蛋白质结构域" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:ProteinDomain .

rd:has_isoform a owl:ObjectProperty ;
    rdfs:label "has isoform" ;
    rdfs:comment "蛋白质异构体" ;
    rdfs:domain rd:Target ;
    rdfs:range rd:Target .

rd:associated_with_disease a owl:ObjectProperty ;
    rdfs:label "associated with disease" ;
    rdfs:comment "与疾病关联" ;
    rdfs:domain rd:Target ;
    rdfs:range clinical:Disease .

# ----- Target 辅助类 -----

rd:TargetType a owl:Class ;
    rdfs:label "Target Type" ;
    rdfs:comment "靶点类型" .

rd:TargetType_receptor a rd:TargetType ;
    rdfs:label "Receptor" ;
    rdfs:comment "受体" .

rd:TargetType_enzyme a rd:TargetType ;
    rdfs:label "Enzyme" ;
    rdfs:comment "酶" .

rd:TargetType_ion_channel a rd:TargetType ;
    rdfs:label "Ion Channel" ;
    rdfs:comment "离子通道" .

rd:TargetType_transporter a rd:TargetType ;
    rdfs:label "Transporter" ;
    rdfs:comment "转运体" .

rd:TargetType_nuclear_receptor a rd:TargetType ;
    rdfs:label "Nuclear Receptor" ;
    rdfs:comment "核受体" .

rd:TargetType_gprotein_coupled_receptor a rd:TargetType ;
    rdfs:label "G-Protein Coupled Receptor" ;
    rdfs:comment "G蛋白偶联受体" .

rd:TargetType_kinase a rd:TargetType ;
    rdfs:label "Kinase" ;
    rdfs:comment "激酶" .

rd:TargetType_protease a rd:TargetType ;
    rdfs:label "Protease" ;
    rdfs:comment "蛋白酶" .

rd:TargetType_transcription_factor a rd:TargetType ;
    rdfs:label "Transcription Factor" ;
    rdfs:comment "转录因子" .

rd:TargetType_epigenetic_target a rd:TargetType ;
    rdfs:label "Epigenetic Target" ;
    rdfs:comment "表观遗传靶点" .

rd:ProteinDomain a owl:Class ;
    rdfs:label "Protein Domain" ;
    rdfs:comment "蛋白质结构域" .

#===========================================================
# 3. Assay (实验/生物检测)
#===========================================================

rd:Assay a owl:Class ;
    rdfs:label "Assay" ;
    rdfs:subClassOf pharma:Entity ;
    rdfs:comment "生物检测/实验" ;
    owl:equivalentClass bao:BAO_0000176 .  # assay

# ----- Assay 属性 -----

rd:has_assay_type a owl:ObjectProperty ;
    rdfs:label "has assay type" ;
    rdfs:comment "实验类型" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:AssayType .

rd:has_assay_format a owl:ObjectProperty ;
    rdfs:label "has assay format" ;
    rdfs:comment "实验形式" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:AssayFormat .

rd:has_detection_method a owl:ObjectProperty ;
    rdfs:label "has detection method" ;
    rdfs:comment "检测方法" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:DetectionMethod .

rd:has_readout_type a owl:ObjectProperty ;
    rdfs:label "has readout type" ;
    rdfs:comment "读数类型" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:ReadoutType .

rd:has_assay_design a owl:DatatypeProperty ;
