    rdfs:range xsd:decimal .

rd:has_buffer_type a owl:DatatypeProperty ;
    rdfs:label "has buffer type" ;
    rdfs:comment "缓冲液类型" ;
    rdfs:domain rd:Assay ;
    rdfs:range xsd:string .

rd:has_assay_quality a owl:ObjectProperty ;
    rdfs:label "has assay quality" ;
    rdfs:comment "实验质量评估" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:AssayQuality .

# ----- Assay 关系 -----

rd:measures a owl:ObjectProperty ;
    rdfs:label "measures" ;
    rdfs:comment "测量靶点活性" ;
    rdfs:domain rd:Assay ;
    rdfs:range rd:Target .

rd:tests_compound a owl:ObjectProperty ;
