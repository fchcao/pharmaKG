    rdfs:range xsd:string .

rd:has_tissue_type a owl:DatatypeProperty ;
    rdfs:label "has tissue type" ;
    rdfs:comment "组织类型" ;
    rdfs:domain rd:Assay ;
    rdfs:range xsd:string .

rd:has_organism a owl:DatatypeProperty ;
    rdfs:label "has organism" ;
    rdfs:comment "实验生物体" ;
    rdfs:domain rd:Assay ;
    rdfs:range xsd:string .

rd:has_incubation_time a owl:DatatypeProperty ;
    rdfs:label "has incubation time" ;
    rdfs:comment "孵育时间" ;
