    sh:targetClass rd:Target ;
    sh:property [
        sh:path pharma:has_primary_id ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path rd:has_target_type ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ] .

